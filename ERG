' 9. & 10. Copy unique sheets and save
        If dictOutputSheets.Count > 0 Then
            Dim sheetNames() As String
            ReDim sheetNames(0 To dictOutputSheets.Count - 1)
            
            ' Transfer Dictionary keys into a String Array
            Dim keyIdx As Long
            Dim k As Variant
            keyIdx = 0
            For Each k In dictOutputSheets.Keys
                sheetNames(keyIdx) = CStr(k) ' Force cast to String
                keyIdx = keyIdx + 1
            Next k
            
            ' Now sheetNames is equivalent to Array("Sales", "Inventory", "Reports")
            ' Check if all sheets exist before copying
            On Error Resume Next
            wbCalc.Sheets(sheetNames).Select
            If Err.Number <> 0 Then
                MsgBox "One or more sheet names in the Setup tab (Column G) do not exist in the model.", vbCritical
                On Error GoTo 0
                Exit Sub
            End If
            On Error GoTo 0

            ' Execute the copy command
            wbCalc.Sheets(sheetNames).Copy
            Set wbOutput = ActiveWorkbook
            
            ' Save and close
            wbOutput.SaveAs folderPath & "\TC_" & tcID & ".xlsx"
            wbOutput.Close False
        Else
            MsgBox "No valid Output Sheets found in Setup tab Column G.", vbExclamation
            Exit Sub
        End If
